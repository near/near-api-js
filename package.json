{
    "name": "near-api-js",
    "description": "JavaScript library to interact with NEAR Protocol via RPC API",
    "version": "6.5.1",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/near/near-api-js.git"
    },
    "engines": {
        "node": ">=20.18.3",
        "pnpm": ">=10.4.1"
    },
    "packageManager": "pnpm@10.4.1",
    "homepage": "https://github.com/near/near-api-js",
    "type": "module",
    "exports": {
        ".": {
            "import": "./lib/index.js",
            "types": "./lib/index.d.ts"
        }
    },
    "dependencies": {
        "@noble/curves": "1.8.1",
        "@noble/hashes": "1.7.1",
        "@scure/base": "1.2.4",
        "@zorsh/zorsh": "^0.4.0",
        "depd": "2.0.0",
        "exponential-backoff": "3.1.2",
        "is-my-json-valid": "2.20.6",
        "mustache": "4.0.0",
        "secp256k1": "5.0.1"
    },
    "devDependencies": {
        "@arethetypeswrong/cli": "0.18.2",
        "@biomejs/biome": "^2.3.6",
        "@changesets/changelog-github": "0.5.1",
        "@changesets/cli": "2.29.7",
        "@commitlint/cli": "19.3.0",
        "@commitlint/config-conventional": "19.7.1",
        "@types/node": "20.0.0",
        "@vitest/ui": "4.0.10",
        "commitlint": "19.7.1",
        "husky": "9.1.7",
        "near-hello": "0.5.1",
        "near-workspaces": "5.0.0",
        "rimraf": "6.0.1",
        "semver": "7.7.1",
        "ts-node": "10.9.2",
        "typedoc": "0.27.9",
        "typescript": "5.4.5",
        "vitest": "4.0.10"
    },
    "keywords": [],
    "license": "(MIT AND Apache-2.0)",
    "scripts": {
        "compile": "tsc -p ./tsconfig.json",
        "dev": "pnpm compile -w",
        "prebuild": "pnpm clean",
        "build": "tsc -p ./tsconfig.json",
        "test": "vitest run test",
        "typecheck": "tsc --noEmit -p ./tsconfig.typecheck.json",
        "lint": "biome check",
        "prefuzz": "pnpm build",
        "fuzz": "jsfuzz test/fuzz/borsh-roundtrip.js test/fuzz/corpus/",
        "clean": "pnpm rimraf lib",
        "check-exports": "attw --pack . --ignore-rules cjs-resolves-to-esm no-resolution",
        "docs:generate": "typedoc",
        "autoclave": "rimraf lib && rimraf node_modules && rimraf coverage && rimraf node_modules && rimraf e2e/node_modules && rimraf e2e/coverage && rimraf e2e/tests/node_modules",
        "release": "changeset publish"
    },
    "files": [
        "lib",
        "dist"
    ],
    "author": "NEAR Inc",
    "sideEffects": false,
    "resolutions": {
        "near-sandbox": "0.0.18"
    }
}
