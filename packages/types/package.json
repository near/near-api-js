{
	"name": "@near-js/types",
	"version": "2.0.1",
	"description": "TypeScript types for working with the Near JS API",
	"main": "lib/commonjs/index.cjs",
	"module": "lib/esm/index.js",
	"types": "./lib/esm/index.d.ts",
	"scripts": {
		"build": "pnpm clean && pnpm build:esm && pnpm build:cjs",
		"clean": "rimraf lib/",
		"build:esm": "swc src --copy-files --out-dir lib/esm --out-file-extension js --strip-leading-paths --config-file ../../swc.esm.json",
		"build:cjs": "swc src --copy-files --out-dir lib/commonjs --out-file-extension cjs --strip-leading-paths --config-file ../../swc.cjs.json",
		"lint": "eslint -c ../../.eslintrc.ts.yml src/**/*.ts",
		"lint:fix": "eslint -c ../../.eslintrc.ts.yml src/**/*.ts --fix"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/node": "20.0.0",
		"build": "workspace:*",
		"jest": "29.7.0",
		"ts-jest": "29.2.6",
		"tsconfig": "workspace:*",
		"typescript": "5.4.5"
	},
	"files": ["lib"],
	"exports": {
		".": {
			"types": "./lib/esm/index.d.ts",
			"import": "./lib/esm/index.js",
			"require": "./lib/commonjs/index.cjs"
		}
	}
}
