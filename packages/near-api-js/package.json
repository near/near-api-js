{
    "name": "near-api-js",
    "description": "JavaScript library to interact with NEAR Protocol via RPC API",
    "version": "5.0.0-next.6",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/near/near-api-js.git"
    },
    "homepage": "https://github.com/near/near-api-js",
    "main": "lib/esm/index.js",
    "browser": "lib/commonjs/browser-index.cjs",
    "types": "lib/esm/index.d.ts",
    "type": "module",
    "dependencies": {
        "@near-js/accounts": "workspace:*",
        "@near-js/crypto": "workspace:*",
        "@near-js/keystores": "workspace:*",
        "@near-js/keystores-browser": "workspace:*",
        "@near-js/keystores-node": "workspace:*",
        "@near-js/providers": "workspace:*",
        "@near-js/signers": "workspace:*",
        "@near-js/transactions": "workspace:*",
        "@near-js/types": "workspace:*",
        "@near-js/utils": "workspace:*",
        "@near-js/wallet-account": "workspace:*",
        "borsh": "1.0.0",
        "depd": "2.0.0",
        "http-errors": "1.7.2",
        "near-abi": "0.1.1"
    },
    "devDependencies": {
        "@types/http-errors": "1.6.1",
        "@types/node": "20.0.0",
        "bs58": "4.0.0",
        "buffer": "6.0.3",
        "build": "workspace:*",
        "bundlewatch": "0.3.1",
        "concurrently": "7.3.0",
        "in-publish": "2.0.0",
        "jest": "29.7.0",
        "localstorage-memory": "1.0.3",
        "near-hello": "0.5.1",
        "near-workspaces": "3.5.0",
        "process": "^0.11.10",
        "rimraf": "6.0.1",
        "semver": "7.1.1",
        "ts-jest": "29.1.5",
        "tsconfig": "workspace:*",
        "util": "^0.12.5"
    },
    "keywords": [],
    "license": "(MIT AND Apache-2.0)",
    "scripts": {
        "compile:esm": "tsc -p ./tsconfig.json",
        "compile:cjs": "tsc -p ./tsconfig.cjs.json && cjsify ./lib/commonjs",
        "dev": "pnpm compile -w",
        "build": "pnpm compile:esm && pnpm compile:cjs",
        "test": "jest --passWithNoTests",
        "lint": "concurrently \"pnpm:lint:*(!fix) --no-error-on-unmatched-pattern\"",
        "lint:src": "eslint --ext .ts src",
        "prefuzz": "pnpm build",
        "fuzz": "jsfuzz test/fuzz/borsh-roundtrip.js test/fuzz/corpus/",
        "clean": "pnpm rimraf lib"
    },
    "files": [
        "lib"
    ],
    "author": "NEAR Inc",
    "exports": {
        "require": "./lib/commonjs/index.cjs",
        "import": "./lib/esm/index.js"
    }
}
