{
    "name": "near-api-js",
    "description": "JavaScript library to interact with NEAR Protocol via RPC API",
    "version": "6.3.1",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/near/near-api-js.git"
    },
    "homepage": "https://github.com/near/near-api-js",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "dependencies": {
        "@near-js/accounts": "workspace:*",
        "@near-js/biometric-ed25519": "workspace:*",
        "@near-js/client": "workspace:*",
        "@near-js/crypto": "workspace:*",
        "@near-js/iframe-rpc": "workspace:*",
        "@near-js/keystores": "workspace:*",
        "@near-js/keystores-browser": "workspace:*",
        "@near-js/keystores-node": "workspace:*",
        "@near-js/providers": "workspace:*",
        "@near-js/signers": "workspace:*",
        "@near-js/tokens": "workspace:*",
        "@near-js/transactions": "workspace:*",
        "@near-js/types": "workspace:*",
        "@near-js/utils": "workspace:*",
        "@hexagon/base64": "2.0.4",
        "@noble/curves": "1.8.1",
        "@noble/hashes": "1.7.1",
        "asn1-parser": "1.1.8",
        "borsh": "1.0.0",
        "buffer": "6.0.3",
        "cbor-js": "^0.1.0",
        "depd": "2.0.0",
        "events": "3.3.0",
        "fido2-lib": "3.4.1",
        "http-errors": "1.7.2",
        "is-my-json-valid": "^2.20.6",
        "lru_map": "0.4.1",
        "near-abi": "0.2.0",
        "node-fetch": "2.6.7",
        "randombytes": "2.1.0",
        "secp256k1": "5.0.1"
    },
    "devDependencies": {
        "@types/depd": "^1.1.37",
        "@types/http-errors": "1.6.1",
        "@types/node": "20.0.0",
        "concurrently": "9.1.2",
        "in-publish": "2.0.0",
        "jest": "29.7.0",
        "near-hello": "0.5.1",
        "near-workspaces": "5.0.0",
        "rimraf": "^6.0.1",
        "semver": "7.7.1",
        "ts-jest": "29.2.6"
    },
    "keywords": [],
    "license": "(MIT AND Apache-2.0)",
    "scripts": {
        "compile": "tsc -p ./tsconfig.json",
        "dev": "pnpm compile -w",
        "build": "tsup",
        "test": "jest test --passWithNoTests",
        "lint": "concurrently \"pnpm:lint:*(!fix) --no-error-on-unmatched-pattern\"",
        "lint:src": "eslint -c ../../.eslintrc.ts.yml --ext .ts src --no-eslintrc",
        "lint:fix": "pnpm lint:src:fix && pnpm lint:test:fix",
        "lint:src:fix": "eslint -c ../../.eslintrc.ts.yml --ext .ts --fix src --no-eslintrc",
        "lint:test:fix": "eslint --ext .js --fix test",
        "prefuzz": "pnpm build",
        "fuzz": "jsfuzz test/fuzz/borsh-roundtrip.js test/fuzz/corpus/",
        "clean": "pnpm rimraf lib",
        "check-exports": "attw --pack ."
    },
    "files": [
        "dist"
    ],
    "author": "NEAR Inc",
    "sideEffects": false
}
