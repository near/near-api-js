{
	"name": "@near-js/iframe-rpc",
	"version": "2.0.1",
	"description": "IFrame RPC client/server implementation",
	"main": "lib/esm/index.js",
	"types": "lib/esm/index.d.ts",
	"type": "module",
	"scripts": {
		"build": "pnpm clean && pnpm build:esm && pnpm build:cjs",
		"clean": "rimraf lib/",
		"build:esm": "swc src --copy-files --out-dir lib/esm --out-file-extension js --strip-leading-paths --config-file ../../swc.esm.json",
		"build:cjs": "swc src --copy-files --out-dir lib/commonjs --out-file-extension cjs --strip-leading-paths --config-file ../../swc.cjs.json"
	},
	"keywords": [],
	"author": "Pagoda",
	"license": "ISC",
	"dependencies": {
		"events": "3.3.0"
	},
	"devDependencies": {
		"@types/node": "18.11.18",
		"build": "workspace:*"
	},
	"exports": {
		".": {
			"types": "./lib/esm/index.d.ts",
			"import": "./lib/esm/index.js",
			"require": "./lib/commonjs/index.cjs"
		}
	}
}
