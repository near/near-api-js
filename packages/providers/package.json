{
    "name": "@near-js/providers",
    "version": "2.5.1",
    "description": "Library of implementations for interfacing with the NEAR blockchain",
    "module": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "lint": "bunx --bun biome check",
        "lint:fix": "bunx --bun biome check --write",
        "test": "bun test",
        "check-exports": "attw --pack . --profile esm-only --no-summary",
        "clean": "rm -rf lib"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@near-js/transactions": "workspace:*",
        "@near-js/types": "workspace:*",
        "@near-js/utils": "workspace:*",
        "@near-js/crypto": "workspace:*",
        "borsh": "1.0.0",
        "exponential-backoff": "^3.1.2"
    },
    "devDependencies": {
        "near-workspaces": "5.0.0",
        "typescript": "5.9.3"
    },
    "peerDependencies": {
        "@near-js/transactions": "^2.0.1",
        "@near-js/types": "^2.0.1",
        "@near-js/utils": "^2.0.1",
        "@near-js/crypto": "^2.0.1"
    },
    "optionalDependencies": {
        "node-fetch": "2.6.7"
    },
    "files": [
        "lib"
    ],
    "exports": {
        ".": {
            "import": "./lib/index.js",
            "types": "./lib/index.d.ts"
        }
    },
    "sideEffects": false
}
