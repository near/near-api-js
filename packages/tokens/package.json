{
	"name": "@near-js/tokens",
	"version": "2.5.1",
	"description": "Modules with tokens",
	"module": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"typesVersions": {
		"*": {
			"ft": [
				"./ft.d.cts"
			],
			"ft/format": [
				"./ft/format.d.cts"
			],
			"nft": [
				"./nft.d.cts"
			],
			"mainnet": [
				"./mainnet.d.cts"
			],
			"testnet": [
				"./testnet.d.cts"
			]
		}
	},
	"type": "module",
	"scripts": {
		"build": "tsc",
		"clean": "rm -rf lib",
		"lint": "eslint -c ../../.eslintrc.ts.yml src/**/*.ts test/**/*.ts --no-eslintrc",
		"lint:fix": "eslint -c ../../.eslintrc.ts.yml src/**/*.ts test/**/*.ts --no-eslintrc --fix",
		"test": "jest",
		"check-exports": "attw --pack ."
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@jest/globals": "^29.7.0",
		"@near-js/types": "workspace:*",
		"@types/node": "20.0.0",
		"jest": "29.7.0",
		"ts-jest": "29.2.6",
		"tsconfig": "workspace:*",
		"typescript": "5.4.5"
	},
	"files": [
		"lib"
	],
	"exports": {
		".": {
			"import": "./lib/index.js",
			"types": "./lib/index.d.ts"
		}
	},
	"sideEffects": false
}
