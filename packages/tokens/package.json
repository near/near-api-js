{
	"name": "@near-js/tokens",
	"version": "2.2.5",
	"description": "Modules with tokens",
	"main": "lib/commonjs/index.cjs",
	"module": "lib/esm/index.js",
	"types": "lib/esm/index.d.ts",
	"typesVersions": {
		"*": {
			"ft": [
				"./ft.d.cts"
			],
			"ft/format": [
				"./ft/format.d.cts"
			],
			"nft": [
				"./nft.d.cts"
			],
			"mainnet": [
				"./mainnet.d.cts"
			],
			"testnet": [
				"./testnet.d.cts"
			]
		}
	},
	"type": "module",
	"scripts": {
		"build": "tsup",
		"clean": "rimraf lib/ ft.js ft.d.cts nft.js nft.d.cts mainnet.js mainnet.d.cts testnet.js testnet.d.cts ft/",
		"lint": "eslint -c ../../.eslintrc.ts.yml src/**/*.ts test/**/*.ts --no-eslintrc",
		"lint:fix": "eslint -c ../../.eslintrc.ts.yml src/**/*.ts test/**/*.ts --no-eslintrc --fix",
		"test": "jest",
		"check-exports": "attw --pack ."
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@jest/globals": "^29.7.0",
		"@near-js/types": "workspace:*",
		"@types/node": "20.0.0",
		"jest": "29.7.0",
		"ts-jest": "29.2.6",
		"tsconfig": "workspace:*",
		"typescript": "5.4.5"
	},
	"files": [
		"lib",
		"ft.js",
		"ft.d.cts",
		"ft",
		"nft.js",
		"nft.d.cts",
		"mainnet.js",
		"mainnet.d.cts",
		"testnet.js",
		"testnet.d.cts"
	],
	"exports": {
		".": {
			"require": "./lib/commonjs/index.cjs",
			"import": "./lib/esm/index.js"
		},
		"./mainnet": {
			"require": "./lib/commonjs/mainnet/index.cjs",
			"import": "./lib/esm/mainnet/index.js"
		},
		"./testnet": {
			"require": "./lib/commonjs/testnet/index.cjs",
			"import": "./lib/esm/testnet/index.js"
		}
	},
	"sideEffects": false
}
