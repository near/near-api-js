        "@types/json-schema": "^7.0.15",
        "@types/node": "20.0.0",
        "bs58": "4.0.0",
        "build": "workspace:*",
        "jest": "29.7.0",
        "near-hello": "0.5.1",
        "near-workspaces": "3.5.0",
        "node-fetch": "2.6.7",
        "semver": "7.1.1",
        "ts-jest": "29.1.5",
