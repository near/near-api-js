{
  "name": "@near-js/biometric-ed25519",
  "description": "JavaScript library to handle webauthn and biometric keys",
  "version": "1.3.2",
  "main": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "pnpm compile:esm && pnpm compile:cjs",
    "compile:esm": "tsc -p tsconfig.json",
    "compile:cjs": "tsc -p tsconfig.cjs.json && cjsify ./lib/commonjs",
    "lint": "eslint -c ../../.eslintrc.ts.yml src/**/*.ts test/**/*.ts --no-eslintrc",
    "lint:fix": "eslint -c ../../.eslintrc.ts.yml src/**/*.ts test/**/*.ts --no-eslintrc --fix",
    "test": "jest"
  },
  "keywords": [],
  "author": "Pagoda",
  "license": "ISC",
  "dependencies": {
    "@hexagon/base64": "2.0.4",
    "@near-js/crypto": "workspace:*",
    "@near-js/utils": "workspace:*",
    "@noble/curves": "1.8.1",
    "@noble/hashes": "1.7.1",
    "asn1-parser": "1.1.8",
    "borsh": "1.0.0",
    "buffer": "6.0.3",
    "build": "workspace:*",
    "cbor-js": "^0.1.0",
    "fido2-lib": "3.4.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "22.15.3",
    "jest": "29.7.0",
    "ts-jest": "29.2.6"
  },
  "exports": {
    "require": "./lib/commonjs/index.cjs",
    "import": "./lib/esm/index.js"
  }
}
