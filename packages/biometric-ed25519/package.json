{
    "name": "@near-js/biometric-ed25519",
    "description": "JavaScript library to handle webauthn and biometric keys",
    "version": "2.5.1",
    "types": "./lib/index.d.ts",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "lint": "bunx --bun biome check",
        "lint:fix": "bunx --bun biome check --write",
        "test": "bun test",
        "check-exports": "attw --ignore-rules cjs-resolves-to-esm internal-resolution-error --pack .",
        "clean": "rm -rf lib"
    },
    "keywords": [],
    "author": "Pagoda",
    "license": "ISC",
    "dependencies": {
        "@hexagon/base64": "2.0.4",
        "@near-js/crypto": "workspace:*",
        "@near-js/utils": "workspace:*",
        "@noble/curves": "1.8.1",
        "@noble/hashes": "1.7.1",
        "asn1-parser": "1.1.8",
        "borsh": "1.0.0",
        "buffer": "6.0.3",
        "cbor-js": "^0.1.0",
        "fido2-lib": "3.4.1"
    },
    "peerDependencies": {
        "@near-js/crypto": "^2.0.1",
        "@near-js/utils": "^2.0.1"
    },
    "exports": {
        ".": {
            "import": "./lib/index.js",
            "types": "./lib/index.d.ts"
        }
    },
    "sideEffects": false,
    "module": "./lib/index.js",
    "files": [
        "lib"
    ]
}
